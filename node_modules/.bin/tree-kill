#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules/tree-kill/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/node_modules:/mnt/e/Project/AI Projects/node_modules:/mnt/e/Project/node_modules:/mnt/e/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules/tree-kill/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/node_modules:/mnt/e/Project/AI Projects/node_modules:/mnt/e/Project/node_modules:/mnt/e/node_modules:/mnt/e/Project/AI Projects/Enterprise Knowledge Assistant/enterprise-knowledge-assistant/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
